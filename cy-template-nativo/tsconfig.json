{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "types": ["node", "cypress"],
    "paths": {
      "cypress-sql-server": ["./node_modules/cypress-sql-server"]
    },
    "preserveValueImports": false
  },
  "include": [
    "cypress/**/*.ts",
    "cypress.config.ts",
    "src/types/**/*.d.ts"
  ]
}
